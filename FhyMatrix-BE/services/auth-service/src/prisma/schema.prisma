generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("AUTH_DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
}

model Ping {
  id        Int      @id @default(autoincrement())
  message   String
  createdAt DateTime @default(now())
}